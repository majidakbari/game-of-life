{
    "name": "majid/game-of-life",
    "description": "Simple CLI application implementing the Conway's game of life",
    "type": "project",
    "require": {
        "symfony/console": "^7.3",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.2",
        "squizlabs/php_codesniffer": "^3.13"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
